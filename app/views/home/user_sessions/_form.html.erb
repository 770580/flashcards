<%= simple_form_for :user, html: { class: '.form-inline' }, url: home_user_sessions_path do |f| %>
  <%= f.input :email, label: I18n.t('user_sessions.email'), input_html: { name: "email" }%>
  <%= f.input :password, label: I18n.t('user_sessions.pass_label'), input_html: { name: "password"} %>
  <%= f.button :submit, I18n.t('user_sessions.submit') %>
<% end %>
  